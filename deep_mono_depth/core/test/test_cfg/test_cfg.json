{
    "name": "test_self-supervised",
    "method": "self-supervised",
    "epochs": 20,
    "optimizer": {
        "algorithm": "adam",
        "learning_rate": 1e-4
    },
    "dataset": {
        "data": "kitti",
        "path": "./data/test/test_data",
        "batch_size": 1,
        "img_height": 320,
        "img_width": 1024
    },
    "network": {
        "depth_network": "unet_resnet18",
        "pose_network": "pose_resnet18",
        "shared_encoder": false
    }
}